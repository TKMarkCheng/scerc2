*======================== Sensitivity analysis: HR + OR Together ===============
preserve
drop if (outcome=="Critical Disease" | outcome=="Moderate Disease" | outcome=="Invasive Ventilation")
replace statistic ="OR" if statistic=="mor"
replace statistic ="HR" if statistic =="mhr"
meta set lnvalue se if (Cohort=="Total Cancer Cohort" & (statistic=="HR" |  statistic =="OR")), random(reml) studylabel(ref) studysize(n) level(95)
replace ref="Chavez-MacGregor 2022 (Total Cancer Cohort) *" if ref=="Chavez-MacGregor 2022 (Total Cancer Cohort)"
drop if ref=="Konermann 2023 (Delta Cancer Cohort)" | ref=="Konermann 2023 (Omicron Cancer Cohort)" | (Ref=="Cai 2021" & statistic!="OR")
quietly meta sum
egen totalwt = sum(_meta_weight)
gen percenttotalwt = 100* _meta_weight / totalwt
bys outcome (id) : egen sgwt = sum(_meta_weight)
gen psgwt = 100 * _meta_weight / sgwt
gen a=""
egen weight = concat(psgwt a), format(%5.2f) punct("%")
meta forestplot _id _plot _esci weight Ca Co statistic, col(weight, format(%5.2f) title("Weight")) col(Ca, title("Cancer" "(n)")) col(Co, title("Non-Cancer" "(n)")) random(reml) level(95) subgroup(outcome) nullrefline transform("OR": exp) nooverall noohet noohom nogsig nogbhom ghetstats1text("Heterogeneity: {&chi}{sup:2}(7) = 2019.7, {it:p < .01}") gwhomtest1text("H{sub:0}: OR = 1, z = 1.02, {it:p = .31}") ghetstats2text("Heterogeneity: {&chi}{sup:2}(16) = 802.0, {it:p < .01}") gwhomtest2text("H{sub:0}: OR = 1, z = 2.10, {it:p = .04}") ghetstats3text("Heterogeneity: {&chi}{sup:2}(24) = 2017.3, {it:p < .01}") gwhomtest3text("H{sub:0}: OR = 1, z = 6.28, {it:p < .01}") ghetstats4text("Heterogeneity: {&chi}{sup:2}(11) = 184.9, {it:p < .01}") gwhomtest4text("H{sub:0}: OR = 1, z = 0.47, {it:p = .64}") noosig markeropts(mcolor(dkgreen%75) msize(medium)) ciopts(lcolor(black) lwidth(medium))  gmarkeropts(mcolor(dkgreen) mlwidth(thick) mfcolor(white%10)) nullrefline(lcolor(red%85)) title(" {bf:Sensitivity Analysis For Total Cancer Cohort versus Controls: Odds/ Hazard Ratio of Adverse Outcomes}", size(large) ring(30)) xlabel(0.25 "1/4" 0.5 "1/2" 1 "1" 2 "2" 4 "4" 8 "8" 16 "16" 32 "32",labsize(small)) crop(0.01 50) subtitle("  "" "" "" "" " "{bf: <-- Greater Odds In Control Patients      Greater Odds In Cancer Patients -->                                                                }", color(maroon) ring(0) size(medsmall)) note("* The cancer cohort was split into recent treatment (n=4,296) and no recent treatment (n=9,991)")
restore

preserve
replace Cohort="Total Haematological" if (Cohort=="Leukaemia" | Cohort=="Lymphoma" | Cohort=="Multiple Myeloma" )
expand 2 if (Cohort=="Breast"| Cohort=="CNS"| Cohort=="Endocrine" | Cohort=="Gynaecological" | Cohort=="Head and Neck" | Cohort=="Hepato-Pancreato-Biliary" | Cohort=="Lower GI" | Cohort=="Upper GI" | Cohort=="Renal" | Cohort=="Sarcoma" | Cohort=="Skin Cancer" | Cohort=="Thoracic" | Cohort=="Urological"), gen(bb)
replace Cohort="Total Solid" if bb==1
drop if  (statistic!="mor" & statistic!="mhr") | (Cohort!="Total Haematological" & Cohort!="Total Solid" & Cohort!="Breast"&  Cohort!="Lower GI" & Cohort!="Thoracic" & Cohort!="Urological" & Cohort!="Metastatic" & Cohort!="Non-Metastatic") | outcome!="Mortality"
drop if bb==1 & (Reference=="Starkey 2023" & ref!="Starkey 2023 (Total Solid)" | Reference=="Leuva 2022" & ref!="Leuva 2022 (Total Solid)" | Reference=="Nolan 2023" & ref!="Nolan 2023 (Non-Metastatic Solid Tumours)" & ref!="Nolan 2023 (Metastatic Solid Tumours)")
gen neworder = 1 if Cohort=="Total Haematological"
replace neworder = 2 if Cohort=="Total Solid"
replace neworder = 3 if Cohort=="Breast"
replace neworder = 4 if Cohort=="Lower GI"
replace neworder = 5 if Cohort=="Thoracic"
replace neworder = 6 if Cohort=="Urological"
replace neworder = 7 if Cohort=="Metastatic"
replace neworder = 8 if Cohort=="Non-Metastatic"
label define cancerorder 1 "Total Haematological" 2 "Total Solid" 3 "â—‹         Breast" 4 "â—‹         Lower GI" 5 "â—‹         Thoracic" 6 "â—‹         Urological" 7 "Metastatic" 8 "Non-Metastatic"
label values neworder cancerorder
replace ref ="â—‹         Starkey 2023 (Breast)" if ref=="Starkey 2023 (Breast)" & neworder==3
replace ref ="â—‹         Nolan 2023 (Breast)" if ref=="Nolan 2023 (Breast)" & neworder==3
replace ref ="â—‹         Abuhelwa 2022 (Breast)" if ref=="Abuhelwa 2022 (Breast)" & neworder==3
replace ref ="â—‹         Starkey 2023 (Lower GI)" if ref=="Starkey 2023 (Lower GI)" & neworder==4
replace ref ="â—‹         Leuva 2022 (Colon)" if ref=="Leuva 2022 (Colon)" & neworder==4
replace ref ="â—‹         Abuhelwa 2022 (Lower GI)" if ref=="Abuhelwa 2022 (Lower GI)" & neworder==4
replace ref ="â—‹         Nolan 2023 (Lower GI)" if ref=="Nolan 2023 (Lower GI)" & neworder==4
