replace statistic="OR" if statistic=="mor"
replace statistic="HR" if statistic=="mhr"
meta forestplot _id _plot _esci weight Ca Co statistic, col(_esci, title("Stat. with 95% CI")) col(statistic, title("Stat.")) col(Ca, title("Ca.""(n)")) col(Co, title("Non-Ca.""(n)")) col(weight, format(%5.2f) title("Weight")) random(reml) level(95) subgroup(neworder) nullrefline transform("OR": exp) markeropts(mcolor(dkgreen%75) msize(medium)) ciopts(lcolor(black) lwidth(medium)) gmarkeropts(mcolor(dkgreen) mlwidth(thick) mfcolor(white%10)) nullrefline(lcolor(red%85)) title(" {bf:Sensitivity Analysis For Major Cancer Subtypes versus Non-Cancer Cohort: Odds/ Hazard Ratio of Mortality}", size(medium) ring(30)) xlabel(0.5 "1/2" 1 "1" 2 "2" 4 "4" 8 "8" 16 "16" 32 "32",labsize(medsmall)) crop(0.3 30) subtitle(" "" "" "" "" " "{bf: <-- Greater Odds In Control Patients     Greater Odds In Cancer Patients -->                              }", color(maroon) ring(0) size(medsmall)) ghetstats1text("Heterogeneity: {&chi}{sup:2}(8) = 330.0, {it:p < .01}") gwhomtest1text("H{sub:0}: OR = 1, z = 4.12, {it:p < .01}") ghetstats2text("Heterogeneity: {&chi}{sup:2}(10) = 575.6, {it:p < .01}") gwhomtest2text("H{sub:0}: OR = 1, z = 3.50, {it:p < .01}") ghetstats3text("â—‹         Heterogeneity: {&chi}{sup:2}(2) = 41.6, {it:p < .01}") gwhomtest3text("â—‹         H{sub:0}: OR = 1, z = 1.67, {it:p = .10}") ghetstats4text("â—‹         Heterogeneity: {&chi}{sup:2}(3) = 41.2, {it:p < .01}") gwhomtest4text("â—‹         H{sub:0}: OR = 1, z = 3.10, {it:p < .01}") ghetstats5text("â—‹         Heterogeneity: {&chi}{sup:2}(5) = 307.6, {it:p < .01}") gwhomtest5text("â—‹         H{sub:0}: OR = 1, z = 4.47, {it:p < .01}") ghetstats6text("â—‹         Heterogeneity: {&chi}{sup:2}(5) = 96.6, {it:p < .01}") gwhomtest6text("â—‹         H{sub:0}: OR = 1, z = 1.01, {it:p = .31}") ghetstats7text("Heterogeneity: {&chi}{sup:2}(2) = 191.7, {it:p < .01}") gwhomtest7text("H{sub:0}: OR = 1, z = 2.10, {it:p = .04}") ghetstats8text("Heterogeneity: {&chi}{sup:2}(3) = 87.9, {it:p < .01}") gwhomtest8text("H{sub:0}: OR = 1, z = 3.82, {it:p < .01}")nooverall nogsig nogbhom noomarker ohomtesttext("H{sub:0}: OR{sub:Haem} = OR{sub:Solid} , {&chi}{sup:2}(1) = 3.34, {it:p = .07}") osigtesttext("H{sub:0}: OR{sub:Solid Subtype} = OR{sub:{&Sigma}(Solid Subtypes)} , {&chi}{sup:2}(4) = 9.41, {it:p = .05}") ohetstatstext("H{sub:0}: OR{sub:Metastatic} = OR{sub:Localised} , {&chi}{sup:2}(1) = 1.06, {it:p = .30}")
restore

