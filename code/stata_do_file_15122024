meta regress lnorlungdisease, fixed
estat bubbleplot, reweighted name(g1, replace) xtitle(ln(Odds Ratio of Chronic Lung Disease)) ytitle(Reported ln(Odds Ratio of Mortality)) title("Meta-Regression: Chronic Lung Disease As A Potential" "Confounding Factor In Mortality", size(medium)) legend(off) text(0.5 -0.65 "ln(OR{sub:Mortality}) = 0.823 - 0.659ln(OR{sub:CLD})" " " "p < .0001") text(1.64 0.4 "Udovica 2022", size(vsmall))
predict residual, residuals
predict fitted, fitted
twoway scatter residual fitted, name(g2, replace) xtitle(Fitted Value) ytitle(Residual) title(Assessment of Heteroscedasticity, size(medium)) note(" ")
graph combine g1 g2, row(1) 
restore

