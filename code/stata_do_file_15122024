meta funnelplot, level(95) random(reml) xtitle("OR of Hospitalisation") ytitle("") b2title("Egger's Test: t=1.03" "{it:p}=0.341 n=8 [n<10]", size(small)) title("") text(0.14 0.76 "Rugge" "2022",size(vsmall)) xlabel(-1.386294 "1/4" -0.693147 "1/2" 0 "1" 0.693147 "2" 1.386294 "4")
graph save hospfunnel, replace
restore
