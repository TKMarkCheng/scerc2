meta set lnvalue se if (Cohort=="Total Cancer Cohort" & statistic =="mor"), fixed studylabel(ref) studysize(n) level(95)
meta regress lnormale, fixed
estat bubbleplot, reweighted name(g1, replace) xtitle(ln(Odds Ratio of Male Sex)) ytitle(Reported ln(Odds Ratio of Mortality)) title("Meta-Regression: Male Sex As A Potential" "Confounding Factor In Mortality", size(medium)) legend(off) text(-0.5 0.3 "ln(OR{sub:Mortality}) = 3.07ln(OR{sub:Male}) - 0.067" " " "p < .0001")
predict residual, residuals
predict fitted, fitted
twoway scatter residual fitted, name(g2, replace) xtitle(Fitted Value) ytitle(Residual) title(Assessment of Heteroscedasticity, size(medium)) note(" ")
graph combine g1 g2, row(1) 
restore

