replace ref ="â—‹         Nolan 2023 (Thoracic)" if ref=="Nolan 2023 (Thoracic)" & neworder==5
replace ref ="â—‹         Abuhelwa 2022 (Thoracic)" if ref=="Abuhelwa 2022 (Thoracic)" & neworder==5
replace ref ="â—‹         Starkey 2023 (Urinary Tract)" if ref=="Starkey 2023 (Urinary Tract)" & neworder==6
replace ref ="â—‹         Starkey 2023 (Penis, Testes or Male Genitalia)" if ref=="Starkey 2023 (Penis, Testes or Male Genitalia)" & neworder==6
replace ref ="â—‹         Starkey 2023 (Prostate)" if ref=="Starkey 2023 (Prostate)" & neworder==6
replace ref ="â—‹         Leuva 2022 (Prostate)" if ref=="Leuva 2022 (Prostate)" & neworder==6
replace ref ="â—‹         Nolan 2023 (Urological)" if ref=="Nolan 2023 (Urological)" & neworder==6
replace ref ="â—‹         Abuhelwa 2022 (Urological)" if ref=="Abuhelwa 2022 (Urological)" & neworder==6
replace ref="â—‹         Dai 2020 (Thoracic)" if ref=="Dai 2020 (Thoracic)" & neworder==5
gsort -enddate ref
meta set lnvalue se, random(reml) studylabel(ref) studysize(n) level(95)
quietly meta sum
egen totalwt = sum(_meta_weight)
gen percenttotalwt = 100* _meta_weight / totalwt
bys Cohort (id) : egen sgwt = sum(_meta_weight)
gen psgwt = 100 * _meta_weight / sgwt
gen a=""
